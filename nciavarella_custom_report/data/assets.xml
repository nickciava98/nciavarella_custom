<odoo>
    <template id="assets_backend" name="css_assets" inherit_id="web.assets_backend">
        <xpath expr="." position="inside">
            <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Courier+Prime"/>
        </xpath>
    </template>

    <template id="report_assets_common" name="report_css_assets" inherit_id="web.report_assets_common">
        <xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Courier+Prime"/>
        </xpath>
    </template>
</odoo>
